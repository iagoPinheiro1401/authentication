{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope ": "off",
        "quotes": ["error", "single"],
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "import/no-extraneous-dependencies": [
            "error",
            {
              "devDependencies": true
            }
          ],
        "no-unreachable": "off",
    }
}
